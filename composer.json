{
    "name": "ataslangit/sistem-informasi-desa",
    "require": {
        "php": ">=7.3",
        "codeigniter/framework": "^3",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "mikey179/vfsstream": "1.6.*",
        "phpunit/phpunit": "4.* || 5.* || 9.*"
    },
    "config": {
        "platform": {
            "php": "7.3"
        },
        "vendor-dir": "donjo-sys",
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "lock": false
    }
}
